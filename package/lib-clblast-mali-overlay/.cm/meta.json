{
  "customize": {
    "extra_dir": "",
    "git_src_dir": "src",
    "install_env": {
      "CLBLAST_BRANCH": "mali-overlay",
      "CLBLAST_URL": "https://github.com/intelfx/CLBlast"
    },
    "use_git_revision": "yes",
    "version": "mali-overlay"
  },
  "deps": {
    "clblast": {
      "local": "yes",
      "name": "CLBlast library (development)",
      "sort": 30,
      "tags": "lib,opencl-blas,clblast,vdev"
    },
    "cmake": {
      "local": "yes",
      "name": "cmake",
      "sort": 5,
      "tags": "tool,cmake"
    },
    "compiler": {
      "local": "yes",
      "name": "C++ compiler",
      "sort": 10,
      "tags": "compiler,lang-cpp"
    },
    "opencl": {
      "local": "yes",
      "name": "OpenCL library",
      "sort": 20,
      "tags": "lib,opencl"
    }
  },
  "end_full_path": {
    "android": "lib$#sep#$libclblast_mali.so",
    "linux": "lib/libclblast_mali.so",
    "mingw": "lib\\libclblas_mali.so",
    "win": "lib\\libclblas_mali.so"
  },
  "need_cpu_info": "yes",
  "only_for_host_os_tags": [
    "windows",
    "linux"
  ],
  "only_for_target_os_tags": [
    "windows",
    "linux"
  ],
  "process_script": "process",
  "soft_uoa": "7b81c6ad862f4b38",
  "suggested_path": "lib-clblast",
  "tags": [
    "lib",
    "blas",
    "no-tune",
    "opencl-blas",
    "clblast",
    "vmali-overlay"
  ]
}
